    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "undici": "6.21.2",
    "uuid": "^10.0.0",
    "vite": "^7.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}