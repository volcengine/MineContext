{
  "name": "MineContext",
  "version": "0.1.3",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "installConfig": {
      "hoistingLimits": [
        "packages/database"
      ]
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "build:externals": "node -e \"if(process.platform!=='win32')require('child_process').execSync('chmod +x build-python.sh')\"",
    "dev": "electron-vite dev",
    "build:dev": "npm run copy-backend && npm run build && electron-vite dev",
    "build": "npm run build:externals && node build-python.js && npm run typecheck && node -e \"if(process.platform==='win32'){require('fs').rmSync('./dist',{recursive:true,force:true})}else{require('child_process').execSync('rm -rf ./dist')}\" && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run copy-backend && npm run build && electron-builder --win",
    "build:mac": "npm run copy-backend && npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "copy-backend": "node scripts/copy-prebuilt-backend.js",
    "electron-pack": "npm run build && electron-builder --mac"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.14",
    "@ai-sdk/react": "^2.0.30",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "ahooks": "^3.9.5",
    "ai": "^5.0.30",
    "allotment": "^1.20.4",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "electron-log": "^5.4.3",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.3.9",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.1.0",
    "jotai": "^2.14.0",
    "lucide-react": "^0.542.0",
    "mitt": "^3.0.1",
    "node-cron": "^4.2.1",
    "node-screenshots": "^0.2.1",
    "os-proxy-config": "^1.1.2",
    "pidusage": "^4.0.1",
    "react-arborist": "^3.4.2",
    "react-syntax-highlighter": "^15.6.6",
    "remove-markdown": "^0.6.2",
    "screenshot-desktop": "^1.15.2",
    "streamdown": "^1.1.10",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@arco-design/web-react": "^2.66.4",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@milkdown/crepe": "^7.15.5",
    "@milkdown/kit": "^7.15.5",
    "@milkdown/react": "^7.15.5",
    "@reduxjs/toolkit": "^2.2.5",
    "@swc/plugin-styled-components": "^7.1.5",
    "@tailwindcss/vite": "^4.1.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/lodash": "^4.17.5",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "latest",
